<%= simple_form_for @dog do |f| %>
  <%= f.input :name %>
  <%= f.input :description, as: :text %>
  <%= f.input :image, input_html: { name: "dog[image][]", multiple: true }, as: :file %>

  <% @dog.images.each do |image| %>
    <%= image_tag @dog.images.first %>
  <% end %>

  <%= f.button :submit %>
<% end %>
